{
  "name": "tokens-manager",
  "version": "1.0.0",
  "description": "Figma Plugin for managing design tokens",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite build --watch --mode development\" \"tsc --watch --project tsconfig.plugin.json\"",
    "build": "vite build && tsc --project tsconfig.plugin.json",
    "build:ui": "vite build",
    "build:plugin": "tsc --project tsconfig.plugin.json"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "^1.98.0",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "sass": "^1.69.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vite-plugin-singlefile": "^0.13.5"
  }
}